
        <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">Home</h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            Presesnt Game List
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
                            <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                                <thead>
                                    <tr>
                                        <th>Room ID</th>
                                        <th>Room Name</th>
                                        <th>Member</th>
                                        <th>Watcher</th>
                                        <th>Creator</th>
                                        <th>Create Time</th>
                                    </tr>
                                </thead>
                                <tbody>

                                    {{#each presentGameList}}
                                        <tr>
                                            <td>{{roomId}}</td>
                                            <td>{{roomName}}</td>
                                            <td>{{currentNumber}} / {{maxNumber}}</td>
                                            <td>{{standbyPlayerCount}}</td>
                                            <td>{{creator}}</td>
                                            <td>{{createTime}}</td>
                                        </tr>
                                    {{/each}}
                                    
                                </tbody>
                            </table>
                            <!-- /.table-responsive -->
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            
        </div>
        <!-- /#page-wrapper -->

        <!-- Page-Level Demo Scripts - Tables - Use for reference -->
        <script>
        $(document).ready(function() {
            $('#dataTables-example').DataTable({
                responsive: true
            });
        });
        </script>
    